{
  "dependencies": {
    "expo": "^51.0.0",
    "expo-router": "^3.5.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.1",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@types/react": "~18.2.14",
    "@types/react-native": "~0.70.6",
    "typescript": "~5.3.3",
    "@testing-library/react-native": "^12.1.0",
    "@types/jest": "^29.5.1",
    "jest": "^29.5.0",
    "jest-expo": "^51.0.0",
    "ts-jest": "^29.1.0",
    "babel-jest": "^29.5.0",
    "eslint": "^8.30.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-native": "^4.0.0"
  },
  "name": "mobile-app-with-test-cases",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.ts,.tsx"
  },
  "jest": {
    "preset": "jest-expo",
    "testEnvironment": "node",
    "coverageDirectory": "./coverage",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover"
    ]
  }
}
